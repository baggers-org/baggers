{
  "name": "@baggers/ui",
  "description": "Web user interface",
  "version": "1.0.0-beta.3",
  "author": "Daniel Cooke <danielcooke1996@gmail.com>",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "apollo",
    "starter"
  ],
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "dev": "next dev",
    "build": "./scripts/build.sh",
    "deploy": "npx serverless",
    "generate": "graphql-let",
    "predev": "yarn generate",
    "prestart": "yarn generate",
    "start": "next start",
    "test": "jest",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write"
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@apollo/client": "3.3.20",
    "@aws-amplify/ui-react": "1.0.6",
    "@babel/preset-env": "7.13.9",
    "@babel/preset-typescript": "7.13.0",
    "@material-ui/core": "4.11.3",
    "@material-ui/icons": "4.11.2",
    "@material-ui/lab": "4.0.0-alpha.58",
    "@material-ui/styles": "4.11.4",
    "@sls-next/serverless-component": "3.3.0",
    "@types/jest": "26.0.23",
    "@types/mongodb": "3.6.8",
    "@types/react-helmet": "6.1.0",
    "@types/react-redux": "7.1.16",
    "@types/styled-components": "5.1.9",
    "@types/uuid": "8.3.0",
    "aws-amplify": "3.3.26",
    "clsx": "1.1.1",
    "deepmerge": "4.2.2",
    "dotenv": "8.2.0",
    "encoding": "0.1.13",
    "graphql": "15.5.0",
    "jest": "27.0.5",
    "lightweight-charts": "3.3.0",
    "lodash": "4.17.21",
    "next": "10.0.7",
    "notistack": "1.0.9",
    "react": "17.0.1",
    "react-cookie": "4.0.3",
    "react-dom": "17.0.1",
    "react-loading-skeleton": "2.2.0",
    "saslprep": "1.0.3",
    "styled-components": "5.2.1",
    "use-debounce": "6.0.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "12.0.1",
    "@commitlint/config-conventional": "12.0.1",
    "@graphql-codegen/cli": "1.21.1",
    "@graphql-codegen/plugin-helpers": "1.18.2",
    "@graphql-codegen/typescript": "1.21.0",
    "@graphql-codegen/typescript-operations": "1.17.14",
    "@graphql-codegen/typescript-react-apollo": "2.2.1",
    "@graphql-codegen/typescript-resolvers": "1.18.2",
    "@next/bundle-analyzer": "11.0.0",
    "@svgr/webpack": "5.5.0",
    "@types/lodash": "4.14.172",
    "@types/node": "14.14.31",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.1",
    "babel-plugin-styled-components": "1.12.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "graphql-let": "0.16.3",
    "lint-staged": "10.5.4",
    "yaml-loader": "0.6.0"
  }
