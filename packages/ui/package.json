{
  "name": "@baggers/ui",
  "description": "Web user interface",
  "version": "1.0.0-beta.7",
  "author": "Daniel Cooke <danielcooke1996@gmail.com>",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "apollo",
    "starter"
  ],
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "dev": "next dev",
    "build": "next build",
    "deploy": "components-v1",
    "generate": "graphql-let",
    "predev": "yarn generate",
    "prestart": "yarn generate",
    "start": "next start",
    "pretest": "rm -rf .nyc_output",
    "ct-dev": "rm -rf .next && npx cypress open-ct",
    "posttest": "npx nyc report --check-coverage",
    "type-check": "tsc",
    "lint": "../../node_modules/.bin/eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "i18n": "i18next 'src/**/*{.ts,.tsx}' -c i18n/i18next-parser.config.js"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@apollo/client": "latest",
    "@aws-amplify/ui-react": "1.0.6",
    "@babel/preset-env": "7.13.9",
    "@babel/preset-typescript": "7.13.0",
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.6.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.1.1",
    "@mui/lab": "^5.0.0-alpha.62",
    "@mui/material": "^5.1.1",
    "@mui/styled-engine": "^5.2.0",
    "@mui/system": "^5.2.0",
    "@mui/x-data-grid-pro": "^5.2.1",
    "@sls-next/serverless-component": "3.3.0",
    "@svgdotjs/svg.js": "^3.1.1",
    "@types/jest": "26.0.23",
    "@types/mongodb": "3.6.8",
    "@types/react-helmet": "6.1.0",
    "@types/react-redux": "7.1.16",
    "@types/uuid": "8.3.0",
    "aws-amplify": "latest",
    "clsx": "1.1.1",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.2.2",
    "deepmerge": "4.2.2",
    "dotenv": "8.2.0",
    "encoding": "0.1.13",
    "graphql": "15.5.0",
    "jest": "27.0.5",
    "lightweight-charts": "3.3.0",
    "lodash": "4.17.21",
    "next": "latest",
    "next-i18next": "^10.0.1",
    "notistack": "latest",
    "react": "17.0.1",
    "react-cookie": "4.0.3",
    "react-dom": "17.0.1",
    "react-loading-skeleton": "2.2.0",
    "saslprep": "1.0.3",
    "use-debounce": "6.0.0",
    "uuid": "8.3.2",
    "vivus": "^0.4.6"
  },
  "devDependencies": {
    "@commitlint/cli": "12.0.1",
    "@commitlint/config-conventional": "12.0.1",
    "@cypress/code-coverage": "^3.9.11",
    "@cypress/react": "^5.10.3",
    "@cypress/webpack-dev-server": "^1.7.0",
    "@graphql-codegen/cli": "1.21.1",
    "@graphql-codegen/plugin-helpers": "1.18.2",
    "@graphql-codegen/typescript": "1.21.0",
    "@graphql-codegen/typescript-operations": "1.17.14",
    "@graphql-codegen/typescript-react-apollo": "2.2.1",
    "@next/bundle-analyzer": "11.0.0",
    "@svgr/webpack": "5.5.0",
    "@testing-library/cypress": "^8.0.2",
    "@types/lodash": "4.14.172",
    "@types/node": "14.14.31",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.1",
    "babel-plugin-istanbul": "^6.1.1",
    "babel-plugin-styled-components": "1.12.0",
    "cypress": "^9.1.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "graphql-let": "0.16.3",
    "html-webpack-plugin": "4",
    "i18next-parser": "^5.3.0",
    "lint-staged": "10.5.4",
    "typescript": "^4.5.5",
    "webpack": "4",
    "webpack-dev-server": "^4.5.0",
    "yaml-loader": "0.6.0"
  },
  "nyc": {
    "all": true,
    "include": "src",
    "branches": 90,
    "lines": 90,
    "functions": 90,
    "statements": 90
  }
}
