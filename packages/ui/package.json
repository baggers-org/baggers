{
  "name": "@baggers/ui",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "cross-env NODE_ENV=production NODE_OPTIONS='-r ./esbuild-overrides'  remix build",
    "prebuild": "yarn generate",
    "deploy": "fly deploy --remote-only",
    "dev": "cross-env NODE_ENV=development NODE_OPTIONS='-r ./esbuild-overrides' remix dev",
    "predev": "nodemon --watch src/graphql -e .gql scripts/mergeGraphQLDocuments.js & yarn generate",
    "lint": "eslint app",
    "postinstall": "remix setup node",
    "start": "cross-env NODE_ENV=production remix-serve build",
    "generate": "graphql-codegen --config codegen.yml",
    "env:staging": "cat .env.staging | flyctl -a baggers-staging secrets import",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.6.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.6.0",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript-graphql-request": "^4.4.1",
    "@loadable/component": "^5.15.2",
    "@mui/icons-material": "^5.1.1",
    "@mui/lab": "^5.0.0-alpha.62",
    "@mui/material": "^5.1.1",
    "@mui/styled-engine": "^5.2.0",
    "@mui/system": "^5.2.0",
    "@mui/x-data-grid-pro": "^5.2.1",
    "@remix-run/node": "^1.2.3",
    "@remix-run/react": "^1.1.3",
    "@remix-run/serve": "^1.2.3",
    "@remix-run/server-runtime": "^1.2.3",
    "@svgdotjs/svg.js": "^3.1.2",
    "cross-env": "^7.0.3",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.3.0",
    "esbuild-plugin-svgr": "^1.0.0",
    "graphql": "^16.3.0",
    "graphql-request": "^4.0.0",
    "i18next": "^21.6.11",
    "lightweight-charts": "^3.8.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.15.3",
    "remix-auth": "^3.2.1",
    "remix-auth-auth0": "1.3.2",
    "remix-i18next": "^2.2.0",
    "use-debounce": "^7.0.1",
    "vivus": "^0.4.6"
