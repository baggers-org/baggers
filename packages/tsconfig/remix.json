{
	"extends": "./base.json",
	"compilerOptions": {
		"lib": [
			"DOM",
			"DOM.Iterable",
			"ES2019"
		],
		"outDir": "build",
		"isolatedModules": true,
		"esModuleInterop": true,
		"jsx": "react-jsx",
		"declaration": true,
		"moduleResolution": "node",
		"module": "CommonJs",
		"resolveJsonModule": true,
		"target": "ES2019",
		"strict": true,
		"allowJs": true,
		"strictPropertyInitialization": false,
		"forceConsistentCasingInFileNames": true
	}
}