{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/api-portfolios/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/api-portfolios/**/*.ts"
        ]
      }
    },
    "type-check": {
      "executor": "./tools/executors/ts-check:ts-check"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/api-portfolios"
      ],
      "options": {
        "jestConfig": "libs/api-portfolios/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}