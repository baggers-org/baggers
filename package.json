{
  "name": "baggers",
  "version": "1.0.0",
  "private": true,
  "description": "Baggers monorepo",
  "main": "null",
  "scripts": {
    "deploy": "./scripts/deploy.sh",
    "test": "FORCE_COLOR=1 yarn workspaces foreach -vApi run test",
    "lint": "FORCE_COLOR=1 yarn workspaces foreach -v run lint",
    "lint:ts-package": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js)\"",
    "build": "FORCE_COLOR=1 yarn workspaces foreach -vtApi run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/dan-cooke/baggers.git"
  },
  "author": "Daniel Cooke <danielcooke1996@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/baggers-ltd/baggers/issues"
  },
  "homepage": "https://github.com/baggers-ltd/baggers#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "4.15.2",
    "@typescript-eslint/parser": "4.15.2",
    "eslint": "7.20.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "lerna": "^4.0.0",
    "prettier": "2.2.1",
    "typescript": "4.2.2"
  },
  "workspaces": {
    "packages": [
      "packages/**"
    ]
  },
  "resolutions": {
    "@lerna/package": "patch:@lerna/package@npm:4.0.0#.yarn-patches/@lerna/package.patch",
    "@lerna/package-graph": "patch:@lerna/package-graph@npm:4.0.0#.yarn-patches/@lerna/package-graph.patch"
  }
}
